{
  "$schema": "vscode://schemas/color-theme",
  "type": "Spacemacs Dark",
  "colors": {
    "debugToolBar.background": "#0a0814",
    "activityBar.background": "#212026",
    "activityBar.foreground": "#cbc1d5",
    "activityBarBadge.background": "#222226",
    "activityBarBadge.foreground": "#cbc1d5",
    "contrastBorder": "#292b2e",
    "focusBorder": "#5d4d7a",
    "dropdown.background": "#212026",
    "dropdown.border": "#5d4d7a",
    "dropdown.foreground": "#cbc1d5",
    "editor.background": "#292b2e",
    "editor.foreground": "#cbc1d5",
    "editor.lineHighlightBackground": "#212026",
    "editor.selectionBackground": "#5d4d7a",
    "editorBracketMatch.background": "#5d4d7a",
    "editorBracketMatch.border": "#686868",
    "editorCursor.foreground": "#e3dedd",
    "editorGroupHeader.noTabsBackground": "#292b2e",
    "editorGroupHeader.tabsBackground": "#292b2e",
    "editorLineNumber.foreground": "#5d4d7a",
    "list.activeSelectionBackground": "#100a14",
    "list.activeSelectionForeground": "#b2b2b2",
    "list.dropBackground": "#100a14",
    "list.focusBackground": "#100a14",
    "list.highlightForeground": "#686868",
    "list.hoverBackground": "#100a14",
    "list.inactiveSelectionBackground": "#212026",
    "sideBar.background": "#212026",
    "sideBarSectionHeader.background": "#212026",
    "sideBarTitle.foreground": "#cbc1d5",
    "statusBar.background": "#222226",
    "statusBar.debuggingBackground": "#5d4d7a",
    "statusBar.foreground": "#cbc1d5",
    "statusBar.noFolderBackground": "#222226",
    "statusBarItem.activeBackground": "#827591",
    "statusBarItem.hoverBackground": "#827591",
    "statusBarItem.prominentBackground": "#827591",
    "statusBarItem.prominentHoverBackground": "#827591",
    "tab.activeBackground": "#5d4d7a",
    "tab.activeForeground": "#cbc1d5",
    "tab.border": "#5d4d7a",
    "tab.inactiveBackground": "#212026",
    "tab.inactiveForeground": "#a094a2",
    "terminal.ansiBlack": "#b2b2b2",
    "terminal.ansiBlue": "#3a81c3",
    "terminal.ansiBrightBlack": "#b2b2b2",
    "terminal.ansiBrightBlue": "#d7dfff",
    "terminal.ansiBrightCyan": "#28def0",
    "terminal.ansiBrightGreen": "#86dc2f",
    "terminal.ansiBrightMagenta": "#a31db1",
    "terminal.ansiBrightRed": "#f2241f",
    "terminal.ansiBrightWhite": "#b2b2b2",
    "terminal.ansiBrightYellow": "#b1951d",
    "terminal.ansiCyan": "#21b8c7",
    "terminal.ansiGreen": "#67b11d",
    "terminal.ansiMagenta": "#800080",
    "terminal.ansiRed": "#ba2f59",
    "terminal.ansiWhite": "#b2b2b2",
    "terminal.ansiYellow": "#b1951d",
    "titleBar.activeBackground": "#212026",
    "widget.shadow": "#5d4d7a"
  },
  "tokenColors": [
    {
      "scope": ["markup.heading.1.org", "heading.1"],
      "settings": {
        "foreground": "#4f97d7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading.2.org", "heading.2"],
      "settings": {
        "foreground": "#2d9574",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading.3.org", "heading.3"],
      "settings": {
        "foreground": "#67b11d"
      }
    },
    {
      "scope": ["markup.heading.4.org", "heading.4"],
      "settings": {
        "foreground": "#b1951d"
      }
    },
    {
      "scope": ["markup.heading.5.org", "heading.5"],
      "settings": {
        "foreground": "#4f97d7"
      }
    },
    {
      "scope": ["markup.heading.6.org", "heading.6"],
      "settings": {
        "foreground": "#2d9574"
      }
    },
    {
      "scope": "punctuation.definition.heading.org",
      "settings": {
        "foreground": "#4f97d7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.other.todo.org",
      "settings": {
        "foreground": "#dc752f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.other.priority.org",
      "settings": {
        "foreground": "#dc752f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.tag.org",
      "settings": {
        "foreground": "#9f8766",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.other.progress.org",
      "settings": {
        "foreground": "#a45bad"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.org",
      "settings": {
        "foreground": "#4f97d7"
      }
    },
    {
      "scope": "constant.language.checkbox.org",
      "settings": {
        "foreground": "#7590db",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.tag.description.term.org",
      "settings": {
        "foreground": "#c56ec3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.separator.key-value.org",
      "settings": {
        "foreground": "#9f8766",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.numeric.list-counter.org",
      "settings": {
        "foreground": "#a45bad"
      }
    },
    {
      "scope": "entity.name.function.org",
      "settings": {
        "foreground": "#9f8766",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.unquoted.org",
      "settings": {
        "foreground": "#bc6ec5"
      }
    },
    {
      "scope": "markup.bold.org",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic.org",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.underline.org",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.strikethrough.org",
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#dc322f"
      }
    },
    {
      "scope": "markup.inline.raw.org",
      "settings": {
        "foreground": "#28def0"
      }
    },
    {
      "scope": "markup.underline.link.org",
      "settings": {
        "foreground": "#4f97d7",
        "fontStyle": "underline"
      }
    },
    {
      "settings": {
        "foreground": "#6688cc"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#2aa1ae"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#2d9574"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#a45bad"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "foreground": "#a45bad"
      }
    },
    {
      "name": "User-defined constants",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#a45bad"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#7590db",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Class names",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#bc6ec5"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#bc6ec5"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "#7590db"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Tag start/end",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#2aa1ae"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#bc6ec5"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bc6ec5"
      }
    },
    {
      "name": "Library constants",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#a45bad"
      }
    },
    {
      "name": "Library classes/types",
      "scope": ["support.type", "support.class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#e0211d"
      }
    },
    {
      "name": "diff: header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#ddbb88"
      }
    },
    {
      "name": "Source Java Storage Modifier Import",
      "scope": [
        "source.java storage.modifier.import",
        "source.java storage.modifier.package",
        "source.java entity.name.type.class",
        "source.java storage.type",
        "source.java meta.definition.class.implemented.interfaces",
        "source.java entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Java Method Body",
      "scope": ["meta.method.body.java", "meta.function-call.java"],
      "settings": {
        "foreground": "#cbc1d5ff"
      }
    },
    {
      "name": "Java Operators",
      "scope": [
        "source.java keyword.operator.assignment",
        "source.java keyword.operator.arithmetic",
        "source.java storage.type.function.arrow.java",
        "source.java punctuation.definition.parameters.begin.bracket.round",
        "source.java punctuation.definition.parameters.end.bracket.round"
      ],
      "settings": {
        "foreground": "#cbc1d5ff"
      }
    },
    {
      "name": "Java Curly Braces",
      "scope": [
        "source.java begin.bracket.curly",
        "source.java end.bracket.curly"
      ],
      "settings": {
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Java Constructor",
      "scope": "source.java entity.name.function meta",
      "settings": {
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Javadoc keywords",
      "scope": "source.java keyword.other.documentation.javadoc.java",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "C++ Directives",
      "scope": ["source.cpp keyword.control.directive"],
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "C++ Keywords",
      "scope": ["source.cpp storage.type", "source.cpp storage.modifier"],
      "settings": {
        "foreground": "#4f97d7"
      }
    },
    {
      "name": "Clojure locals",
      "scope": "source.clojure meta.symbol",
      "settings": {
        "foreground": "#cbc1d5ff"
      }
    },
    {
      "name": "Clojure namespace",
      "scope": [
        "source.clojure entity.global",
        "source.clojure meta.definition.global"
      ],
      "settings": {
        "foreground": "#ce537a"
      }
    },
    {
      "name": "Clojure keyword",
      "scope": "source.clojure constant.keyword",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Clojure namespace",
      "scope": [
        "source.clojure meta.symbol meta.expression",
        "source.clojure meta.vector"
      ],
      "settings": {
        "foreground": "#ce537a"
      }
    }
  ]
}
